{
  "manifest_version": 2,

  "name": "Shock",
  "description": "Shock",
  "version": "0.0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["http://www.google.com/*"],
    "js": ["jquery.min.js"]
  }],
  "permissions": [
    "activeTab",
    "https://raw.githubusercontent.com/.*",
    "https://code.jquery.com/.*"
  ]
}
